  file = {Full Text PDF:/Users/malevy/Library/Application Support/Zotero/Profiles/vpg7jtxl.default/zotero/storage/RN6GHEPK/Green - 1997 - Explaining and envisaging an ecological phenomenon.pdf:application/pdf;Snapshot:/Users/malevy/Library/Application Support/Zotero/Profiles/vpg7jtxl.default/zotero/storage/V7ZXZH4M/abstract.html:text/html}
}

@article{white_common-sense_1995,

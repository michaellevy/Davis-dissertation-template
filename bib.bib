  journal = {Policy Studies Journal},
  author = {Sandstr{\"o}m, Annica and Carlsson, Lars},
  month = nov,
  year = {2008},
  keywords = {network performance,network structure,policy analysis,policy networks,SNA,social network analysis},
  pages = {497--524},
  file = {Full Text PDF:/Users/malevy/Library/Application Support/Zotero/Profiles/vpg7jtxl.default/zotero/storage/MG52I5VI/Sandström and Carlsson - 2008 - The Performance of Policy Networks The Relation b.pdf:application/pdf;Snapshot:/Users/malevy/Library/Application Support/Zotero/Profiles/vpg7jtxl.default/zotero/storage/SG6XIW9A/full.html:text/html}

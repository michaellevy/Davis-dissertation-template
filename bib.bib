@book{meadows_thinking_2008,
  address = {White River Junction, Vt},
  title = {Thinking in {{Systems}}: {{A Primer}}},
  isbn = {978-1-60358-055-7},
  shorttitle = {Thinking in {{Systems}}},
  abstract = {In the years following her role as the lead author of the international bestseller, Limits to Growth\rule{1em}{1pt}the first book to show the consequences of unchecked growth on a finite planet\rule{1em}{1pt} Donella Meadows remained a pioneer of environmental and social analysis until her untimely death in 2001.  Thinking in Systems, is a concise and crucial book offering insight for problem solving on scales ranging from the personal to the global. Edited by the Sustainability Institute's Diana Wright, this essential primer brings systems thinking out of the realm of computers and equations and into the tangible world, showing readers how to develop the systems-thinking skills that thought leaders across the globe consider critical for 21st-century life.  Some of the biggest problems facing the world\rule{1em}{1pt}war, hunger, poverty, and environmental degradation\rule{1em}{1pt}are essentially system failures. They cannot be solved by fixing one piece in isolation from the others, because even seemingly minor details have enormous power to undermine the best efforts of too-narrow thinking.  While readers will learn the conceptual tools and methods of systems thinking, the heart of the book is grander than methodology. Donella Meadows was known as much for nurturing positive outcomes as she was for delving into the science behind global dilemmas. She reminds readers to pay attention to what is important, not just what is quantifiable, to stay humble, and to stay a learner.  In a world growing ever more complicated, crowded, and interdependent, Thinking in Systems helps readers avoid confusion and helplessness, the first step toward finding proactive and effective solutions.},
  language = {English},
  timestamp = {2017-03-29T18:01:01Z},
  publisher = {{Chelsea Green Publishing}},
  author = {Meadows, Donella H.},
  editor = {Wright, Diana},
  month = dec,
  year = {2008}
}

